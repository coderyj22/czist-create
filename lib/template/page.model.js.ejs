import { 
  GetList, 
  GetEntity, 
  AddEntity, 
  UpdateSave, 
  SetDisable, 
  SetEnable 
} from "@/services/<%= data.lowerName%>";


const Model = {
  nameSpace: "<%= data.lowerName%>",
  state: {},
  effects: {
    *getList({ payload }, { call }) {
      const result = yield call(GetList, payload);
      return result;
    },
    *addEntity({ payload },{ call }) {
      const result = yield call(AddEntity, payload);
      return result;
    },
    *getEntity({ payload },{ call }) {
      const result = yield call(GetEntity, payload);
      return result;
    },
    *updateSave({ payload }, { call }) {
      const result = yield call(UpdateSave, payload);
      return result;
    },
    *setDisable({ payload },{ call }) {
      const result = yield call(SetDisable, payload);
      return result;
    },
    *setEnable({ payload },{ call }) {
      const result = yield call(SetEnable, payload);
      return result;
    }
  },
  reducers:{
    updateState(state, {payload}) {
      return {
        ...state,
        ...payload
      }
    }
  }
};

export default Model;
